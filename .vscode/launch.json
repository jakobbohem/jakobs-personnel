  "version": "0.2.0",
  "configurations": [

    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "List all entries",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": [
        "--list"
      ]
    },
    {
      "name": "List available roles",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": [
        "--roles"
      ]
    },
    {
      "name": "default (global) search",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": [
        "Jakob "
      ]
    },
    {
      "name": "extract data (update db)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/extract_data.py",
      "args": []
    },
    {
      "name": "PLOT (craft)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": ["--graph=craft"]
    }
  ]
}