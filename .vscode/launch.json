  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: test lookup.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "escapeArgsForShell":false,
      "args": [
        "--github-prs",
        "Gates Matt"
      ]
    },
    {
      "name": "List and copy team email",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": ["craft=Code", "employer=Double11"
        , "--list"
        , "-cc"
        , "-m"
      ]
    },
    {
      "name": "Analyze (roles)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": [
        "roles", "--craft=code"
      ]
    },
    {
      "name": "default (global) search",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": [
        "Jakob "
      ]
    },
    {
      "name": "extract data (update db)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/extract_data.py",
      "args": []
    },
    {
      "name": "Update (data & properties)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/bootstrap.py",
      "args": ["--noextract"]
    },
    {
      "name": "github commits/PR",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": ["Gabriel Mene", "-gh"]
    },
    {
      "name": "PLOT (craft)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": ["personnel", "world", "--graph=craft"]
    },
    {
      "name": "Analyze (github commits)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": ["github", "gameplay", "--graph=graph"]
    } 
  ]
}