  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: test lookup.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "escapeArgsForShell":false,
      "args": [
        "--github-prs",
        "Gates Matt"
      ]
    },
    {
      "name": "List all entries",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": [
        "--list"
      ]
    },
    {
      "name": "Analyze (roles)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": [
        "roles", "--craft=code"
      ]
    },
    {
      "name": "default (global) search",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/lookup.py",
      "args": [
        "Jakob "
      ]
    },
    {
      "name": "extract data (update db)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/extract_data.py",
      "args": []
    },
    {
      "name": "process Excel (test)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/process_excel.py",
      "args": []
    },
    {
      "name": "PLOT (craft)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": ["personnel", "--graph=craft"]
    },
    {
      "name": "Analyze (github commits)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/analyze.py",
      "args": ["github", "gameplay", "--graph=graph"]
    } 
  ]
}